#pkk_dead_ends {
  [zoom >= 13] {
    marker-file: url('symbols/dead_end_sign.svg');
    marker-width: 10;
    marker-allow-overlap:true;
  }
  [zoom >= 15] {
    marker-file: url('symbols/dead_end_sign.svg');
    marker-width: 20;
    marker-allow-overlap:true;
  }
  [zoom >= 16] {
    marker-file: url('symbols/dead_end_sign.svg');
    marker-width: 25;
    marker-allow-overlap:true;
  }
  [zoom >= 17] {
    marker-file: url('symbols/dead_end_sign.svg');
    marker-width: 35;
    marker-allow-overlap:true;
  }
}

